<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.hambayo248893" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Console">
        <param name="wp-package" value="DebugConsole" />
    </feature>
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <name>STLM Basic</name>
    <description>
      STLM Mobile application for municipal information.
    </description>
    <author email="marius@msft.co.za" href="http://www.msft.co.za/">
      Mariusoft
    </author>
    <content src="index.html" />
