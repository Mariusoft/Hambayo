<?xml version='1.0' encoding='utf-8'?>
<widget id="za.co.msft.hambayo248893" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Console">
        <param name="wp-package" value="DebugConsole" />
    </feature>
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <feature name="InAppBrowser">
        <param name="wp-package" value="InAppBrowser" />
    </feature>
    <name>Hambayo</name>
    <description>
        STLM Mobile application for municipal information.
    </description>
